#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x55e131677d90 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55e131677f20 .scope module, "testbench" "testbench" 3 1;
 .timescale 0 0;
P_0x55e131679620 .param/l "ADDR_WIDTH" 0 3 4, +C4<00000000000000000000000000100000>;
P_0x55e131679660 .param/l "DATA_WIDTH" 0 3 3, +C4<00000000000000000000000000100000>;
P_0x55e1316796a0 .param/l "MAIN_FRE" 0 3 5, +C4<00000000000000000000000001100100>;
v0x55e13169eac0_0 .var "addr", 31 0;
v0x55e1316c4d00_0 .var "data", 31 0;
v0x55e1316c4de0_0 .var "sys_clk", 0 0;
v0x55e1316c4eb0_0 .var "sys_rst", 0 0;
E_0x55e1316b1480 .event posedge, v0x55e1316c4de0_0;
S_0x55e1316b4a60 .scope module, "u_hello" "hello" 3 33, 4 1 0, S_0x55e131677f20;
 .timescale 0 0;
    .scope S_0x55e1316b4a60;
T_0 ;
    %vpi_call/w 4 4 "$display", "Hello World!!!" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55e131677f20;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e1316c4de0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e1316c4eb0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55e1316c4d00_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55e13169eac0_0, 0, 32;
    %end;
    .thread T_1, $init;
    .scope S_0x55e131677f20;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55e1316c4de0_0;
    %inv;
    %store/vec4 v0x55e1316c4de0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55e131677f20;
T_3 ;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e1316c4eb0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55e131677f20;
T_4 ;
    %wait E_0x55e1316b1480;
    %load/vec4 v0x55e1316c4eb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55e13169eac0_0, 0, 32;
    %jmp T_4.1;
T_4.0 ;
    %load/vec4 v0x55e13169eac0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55e13169eac0_0, 0, 32;
T_4.1 ;
    %jmp T_4;
    .thread T_4;
    .scope S_0x55e131677f20;
T_5 ;
    %wait E_0x55e1316b1480;
    %load/vec4 v0x55e1316c4eb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55e1316c4d00_0, 0, 32;
    %jmp T_5.1;
T_5.0 ;
    %load/vec4 v0x55e1316c4d00_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55e1316c4d00_0, 0, 32;
T_5.1 ;
    %jmp T_5;
    .thread T_5;
    .scope S_0x55e131677f20;
T_6 ;
    %vpi_call/w 3 37 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call/w 3 38 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55e131677f20 {0 0 0};
    %delay 50000, 0;
    %vpi_call/w 3 39 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
